{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "components": "./src/components",
          "Pages": "./src/Pages",
          "assets": "./src/assets"
        }
      }
    ],
    "@babel/plugin-transform-runtime"
  ]
}
