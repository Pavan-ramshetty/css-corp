{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "components": "./src/components",
          "Pages": "./src/Pages",
          "assets": "./src/assets",
          "types": "./src/types",
          "utils": "./src/utils",
          "context": "./src/context",
          "hooks": "./src/hooks",
          "reducers": "./src/reducers",
          "actions": "./src/actions",
          "sagas": "./src/sagas",
          "constants": "./src/constants",
          "mobxStore": "./src/mobxStore"
        }
      }
    ],
    "@babel/plugin-transform-runtime"
  ],
  "assumptions": {
    "setPublicClassFields": false
  }
}
